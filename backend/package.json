{
  "name": "ridesharex-backend",
  "version": "1.0.0",
  "description": "RideShare Backend API",
  "main": "dist/app.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/server.js",
    "dev": "nodemon src/server.ts",
    "dev:fallback": "node start-with-fallback.js",
    "dev:env": "node start-with-env.js",
    "start:simple": "node start-server.js",
    "test:api": "node test-api.js",
    "test": "cross-env NODE_ENV=test vitest run",
    "test:all": "cross-env NODE_ENV=test vitest",
    "test:watch": "cross-env NODE_ENV=test vitest --watch",
    "test:coverage": "cross-env NODE_ENV=test vitest run --coverage",
    "lint": "eslint src --ext .ts --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint src --ext .ts --fix",
    "format": "prettier --write \"src/**/*.{ts,js,json}\"",
    "format:check": "prettier --check \"src/**/*.{ts,js,json}\"",
    "db:migrate": "npx sequelize-cli db:migrate",
    "db:seed": "npx sequelize-cli db:seed:all",
    "db:setup": "node scripts/setup-db.js",
    "db:setup:production": "node scripts/setup-production-db.js",
    "db:create-admin": "node scripts/create-admin-user.js",
    "db:seed-demo": "node scripts/seed-demo-data.js",
    "db:test": "node scripts/test-db-connection.js",
    "admin:setup": "node scripts/setup-admin.js",
    "admin:fix": "node scripts/fix-admin-auth.js",
    "admin:test": "node scripts/test-admin-auth.js",
    "seed": "ts-node src/scripts/seedData.ts",
    "seed:dev": "npm run seed",
    "test:firebase": "node test-firebase-admin.js",
    "test:websocket": "node test-websocket-cors.js",
    "test:cors": "node test-websocket-cors.js",
    "setup:websocket": "node setup-websocket-cors.js",
    "start:unified": "node dist/unified-server.js",
    "dev:unified": "nodemon src/unified-server.ts",
    "setup:db": "node setup-database.js",
    "start:fixed": "node start-server.js",
    "debug": "node debug-backend.js",
    "fix": "node fix-backend-issues.js"
  },
  "keywords": [
    "rideshare",
    "vehicle",
    "rental"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@types/nodemailer": "^7.0.2",
    "@types/qrcode": "^1.5.5",
    "@types/redis": "^4.0.10",
    "@types/speakeasy": "^2.0.10",
    "axios": "^1.12.2",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.21.2",
    "express-rate-limit": "^7.5.1",
    "express-validator": "^7.2.1",
    "firebase-admin": "^13.5.0",
    "helmet": "^7.1.0",
    "isomorphic-dompurify": "^2.28.0",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "node-fetch": "^3.3.2",
    "nodemailer": "^7.0.6",
    "pg": "^8.11.3",
    "pg-hstore": "^2.3.4",
    "qrcode": "^1.5.4",
    "redis": "^4.7.1",
    "sequelize": "^6.35.0",
    "sharp": "^0.34.4",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1",
    "speakeasy": "^2.0.0",
    "sqlite3": "^5.1.7",
    "stripe": "^14.5.0",
    "winston": "^3.18.3",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/compression": "^1.7.2",
    "@types/cors": "^2.8.16",
    "@types/express": "^4.17.21",
    "@types/express-validator": "^2.20.33",
    "@types/morgan": "^1.9.9",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.8.10",
    "@types/pg": "^8.10.7",
    "@types/sharp": "^0.31.1",
    "@types/supertest": "^2.0.16",
    "@types/uuid": "^10.0.0",
    "@types/winston": "^2.4.4",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "cross-env": "^10.0.0",
    "eslint": "^8.55.0",
    "nodemon": "^3.0.1",
    "prettier": "^3.1.0",
    "sequelize-cli": "^6.6.3",
    "supertest": "^6.3.3",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2",
    "vitest": "^0.34.6"
  }
}
