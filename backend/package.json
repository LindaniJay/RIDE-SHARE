{
  "name": "ridesharex-backend",
  "version": "1.0.0",
  "description": "RideShare Backend API",
  "main": "dist/app.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/app.js",
    "dev": "nodemon src/app.ts",
    "test": "NODE_ENV=test vitest run",
    "test:all": "NODE_ENV=test vitest",
    "test:watch": "NODE_ENV=test vitest --watch",
    "db:migrate": "npx sequelize-cli db:migrate",
    "db:seed": "npx sequelize-cli db:seed:all",
    "db:setup": "node scripts/setup-db.js",
    "seed": "ts-node src/scripts/seedData.ts"
  },
  "keywords": [
    "rideshare",
    "vehicle",
    "rental"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.0",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "pg": "^8.11.3",
    "pg-hstore": "^2.3.4",
    "redis": "^4.6.0",
    "sequelize": "^6.35.0",
    "socket.io": "^4.7.4",
    "sqlite3": "^5.1.7",
    "stripe": "^14.5.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/compression": "^1.7.2",
    "@types/cors": "^2.8.16",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/morgan": "^1.9.9",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.8.10",
    "@types/pg": "^8.10.7",
    "@types/supertest": "^2.0.16",
    "nodemon": "^3.0.1",
    "sequelize-cli": "^6.6.3",
    "supertest": "^6.3.3",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2",
    "vitest": "^0.34.6"
  }
}
